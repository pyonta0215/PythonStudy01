target_word = "AAA-BBB_CCCCCCCCCC_DDDD_EE_FF-GG.ext"

DOT = "."
UNDERBAR = "_"
CENTERBAR = "-"

def Main():
    print(target_word)
    print(Right(target_word, 3))
    print(Left(target_word, 3))
    print(Mid(target_word, 3, 5))
    print(InStr(target_word, "C"))
    print(Reverse(target_word))

def Left(string, cut_num):
    return string[:cut_num]

def Right(string, cut_num):
    return string[cut_num:]

def Mid(string, num_before, num_after):
    return string[num_before:num_after]

def InStr(target, query):
    return target.find(query)

def Reverse(target):
    return target[::-1]

Main()
